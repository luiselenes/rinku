@page
@using rinku.Pages.Movimientosmes
@model ActualizarModel

<h1>Modificar informacion de pago de mes </h1>
<<div asp-validation-summary="All"></div>
<form method="POST">
  <div class="form-group">
   
  <div class="form-group">
    <label for="num_empleados">Numero Empleado</label>
    <input type="text" name="num_empleados" class="form-control"
         id="num_empleados" aria-describedby="" value="@Model.Detalles.num_empleados">
  </div>

  <div class="form-group">
    <label for="nombre">Mes</label>
    <input type="text" name="mes" class="form-control"
         id="mes" aria-describedby="" value="@Model.Detalles.mes">
  </div>

  <div class="form-group">
    <label for="catidad_entregas">Paquetes entregados en el mes</label>
    <input type="text" class="form-control"
        name="catidad_entregas" id="catidad_entregas" aria-describedby="" value="@Model.Detalles.catidad_entregas">
  </div>

  <a asp-page="./Index" class="btn btn-info">Regresar</a>
  <button type="submit" class="btn btn-primary">Actualizar</button>
  </div>
</form>